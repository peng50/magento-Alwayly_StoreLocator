<?xml version="1.0"?>
<layout version="1.0.0.0">
    <storelocator_index_index>
        <reference name="root">
            <action method="setTemplate"><template>page/1column.phtml</template></action>
        </reference>
        <reference name="head">
            <action method="addJs"><script>lib/jquery/jquery-1.10.2.min.js</script></action>
            <action method="addItem"><type>skin_js</type><name>js/store.js</name></action>
        </reference>
        <reference name="content">
            <block type="storelocator/store" name="storelocator.stores" template="storelocator/store.phtml" />
        </reference>
    </storelocator_index_index>
</layout>